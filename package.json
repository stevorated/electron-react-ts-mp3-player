{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "main": "src/main/main.js",
    "scripts": {
        "start": "yarn clean && yarn build:ui && yarn build:app",
        "dev": "yarn clean && concurrently \"yarn watch:ui\" \"yarn watch:app\"",
        "build:ui": "yarn clean && webpack --mode development --config webpack.config.ui.js",
        "build:app": "webpack --config ./webpack.config.main.js",
        "watch:ui": "yarn clean && webpack --watch --mode development --config webpack.config.ui.js",
        "watch:app": "webpack --watch --config ./webpack.config.main.js",
        "postinstall": "electron-builder install-app-deps",
        "run:electron": "electron ./dist/electron.js",
        "run:app": "electron ./dist/main/main.js",
        "schema": "tsc --build tsconfig.main.json && node ./dist/services/db/playground.js",
        "schema:watch": "tsc-watch --build tsconfig.main.json --onSuccess \"node ./dist/services/db/playground.js\"",
        "scss": "node-sass scss -o html",
        "clean": "rimraf ./dist"
    },
    "electronWebpack": {
        "main": {
            "extraEntries": [
                "@/preload.js"
            ],
            "sourceDirectory": "src/main",
            "webpackConfig": "dist/bundle.webpackmain.js"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.19.0",
        "compression": "^1.7.4",
        "dayjs": "^1.8.21",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "file-loader": "^5.1.0",
        "fluent-ffmpeg": "^2.1.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-draggable": "4.2.0",
        "react-icons": "^3.9.0",
        "react-modal": "3.11.2",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "rimraf": "^3.0.2",
        "sqlite3": "^4.1.1",
        "style-loader": "1.1.3",
        "ts-node": "^8.6.2",
        "url-loader": "^3.0.0",
        "uuid": "7.0.2",
        "webpack-merge": "4.2.2",
        "webpack-shell-plugin": "0.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@types/axios": "^0.14.0",
        "@types/compression": "^1.7.0",
        "@types/dotenv": "^8.2.0",
        "@types/electron": "^1.6.10",
        "@types/electron-builder": "^2.8.0",
        "@types/express": "^4.17.1",
        "@types/file-loader": "^4.2.0",
        "@types/fluent-ffmpeg": "^2.1.14",
        "@types/less": "^3.0.1",
        "@types/lorem-ipsum": "^2.0.0",
        "@types/node": "^12.7.12",
        "@types/node-sass": "^4.11.0",
        "@types/nodemon": "^1.19.0",
        "@types/react": "^16.9.22",
        "@types/react-dom": "^16.9.5",
        "@types/react-modal": "3.10.5",
        "@types/react-redux": "^7.1.7",
        "@types/source-map-support": "^0.5.1",
        "@types/sqlite3": "^3.1.6",
        "@types/uuid": "7.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.6",
        "concurrently": "^5.1.0",
        "css-loader": "^3.4.2",
        "electron": "^8.0.1",
        "electron-builder": "^22.3.2",
        "html-webpack-plugin": "^3.2.0",
        "less": "^3.11.1",
